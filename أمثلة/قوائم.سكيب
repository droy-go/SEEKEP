#
# مثال: العمل مع القوائم
# SEEKEP - Smart Expressive Efficient Knowledge Execution Platform
#

# إنشاء قائمة
متغير أرقام = [1، 2، 3، 4، 5]
اطبع("القائمة الأصلية: " + نص(أرقام))

# إضافة عناصر
أضف(أرقام، 6)
أضف(أرقام، 7)
اطبع("بعد الإضافة: " + نص(أرقام))

# الوصول بالفهرس
اطبع("العنصر الأول: " + نص(أرقام[0]))
اطبع("العنصر الأخير: " + نص(أرقام[الطول(أرقام) - 1]))

# إزالة عنصر
اسحب(أرقام)
اطبع("بعد الإزالة: " + نص(أرقام))

# عكس القائمة
اعكس(أرقام)
اطبع("بعد العكس: " + نص(أرقام))

# قائمة نصوص
متغير فواكه = ["تفاح"، "موز"، "برتقال"، "عنب"]
اطبع("\nالفواكه:")
لكل (فاكهة في فواكه) {
    اطبع("- " + فاكهة)
}

# قائمة مختلطة
متغير مختلط = [1، "نص"، صحيح، [1، 2، 3]]
اطبع("\nقائمة مختلطة: " + نص(مختلط))

# فهم القوائم (list comprehension)
متغير مضاعفات = []
لكل (i في المدى(1، 11)) {
    أضف(مضاعفات، i * 2)
}
اطبع("\nمضاعفات 2: " + نص(مضاعفات))
